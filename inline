  {
   "cell_type": "code",
   "execution_count": 100,
   "metadata": {},
   "outputs": [],
   "source": [
    "def add_metadata(metadata, features):\n",
    "    names = [x.split(\"-\") for x in features['sound.files'])]\n",
    "    ids = [int(x[2].replace(\".wav\",'')) for x in names]\n",
    "    ordered_metadata = pd.DataFrame()\n",
    "    for i in ids:\n",
    "        df = metadata[metadata.Recording_ID == i]\n",
    "        ordered_metadata = ordered_metadata.append(df,ignore_index=True)\n",
    "    return ordered_metadata.join(features)"
   ]
  },
  {
   "cell_type": "code",
